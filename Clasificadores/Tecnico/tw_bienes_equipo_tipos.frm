Private Sub BtnGrabar_Click()
  On Error GoTo UpdateErr
  VAR_VAL = "OK"
  Call valida_campos
  If VAR_VAL = "OK" Then
    If VAR_SW = "ADD" Then

        rs_datos!tipo_eqp = txt_codigo   'var_cod
        rs_datos!estado_codigo = "REG"
'        rs_datos!Correl = 0
        MsgBox "Se guardó con éxito, EL REGISTRO : " + (Ado_datos.Recordset!tipo_eqp)
     End If
     rs_datos!tipo_eqp_descripcion = Txt_descripcion.Text
     rs_datos!fecha_registro = Date
     rs_datos!usr_codigo = glusuario
     rs_datos.UpdateBatch adAffectAll
    
     If VAR_SW = "MOD" Then
       VAR_COD2 = Ado_datos.Recordset!tipo_eqp   'Codigo Llave de la Tabla
     End If
     Call ABRIR_TABLA
     If (dg_datos.SelBookmarks.Count <> 0) Then
        dg_datos.SelBookmarks.Remove 0
     End If
     If Ado_datos.Recordset.RecordCount > 0 Then
        rs_datos.Find "tipo_eqp = '" & VAR_COD2 & "' ", , , 1
        dg_datos.SelBookmarks.Add (rs_datos.Bookmark)
     Else
        rs_datos.MoveLast
     End If
      
      Fra_ABM.Enabled = False
      fraOpciones.Visible = True
      FraGrabarCancelar.Visible = False
      dg_datos.Enabled = True
      txt_codigo.Enabled = True
  End If
  Exit Sub
UpdateErr:
  MsgBox Err.Description
End Sub

Private Sub valida_campos()
  If txt_codigo.Text = "" Then
    MsgBox "Debe registrar el " + lbl_codigo.Caption, vbCritical + vbExclamation, "Validación de datos"
    VAR_VAL = "ERR"
    Exit Sub
  End If
  If Txt_descripcion.Text = "" Then
    MsgBox "Debe registrar la " + lbl_descripcion.Caption, vbCritical + vbExclamation, "Validación de datos"
    VAR_VAL = "ERR"
    Exit Sub
  End If
End Sub

